---
layout: ./base.vto
containerClass: ceo
---
<section class="ceo" id="ceo">
  <div class="inner">
    <h2 class="">{{ title }}</h2>
    <div class="name new-title">
      {{ name }}
    </div>
    <div class="message-photo" style="float:right;">
      <img src="{{ it.img }}" alt="{{ it.imgalt }}" width="200" transform-images="avif webp jpg 200@2">
    </div>
    <div class="profile-main">
      {{ content }}
    </div>

  </div>
</section>
