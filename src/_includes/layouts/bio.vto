---
layout: ./base.vto
containerClass: bio
---
<section class="bio" id="bio-view">
  <div class="inner">
    <div class="bio-view-header">
      <img src="{{ it.img }}" alt="{{ title }}" width="200" height=200>
      <h2>{{ title }}</h2>
      <p>{{ short }}</p>
    </div>
    <div class="txt from-markdown">
      {{ bio |> md }}
    </div>
  </div>
</section>
