              {{ for item of search.pages(`tags*=Advisor type=bio lang=${lang}`, "weight") }}
              <li>
                <a href="{{ item.url }}" class="c-card-member c-card-member--small">
                  <p class="c-card-member__role">{{ item.position }}</p>
                  <p class="c-card-member__name">{{ item.name }}</p>
